program forCycle;

function factorial(n: integer): integer;
var m: integer;
begin
    factorial := 1;
    for i := 1 to n do factorial := factorial * i;
end;

var n, res: integer;

begin
    write('Enter number: ');
    readln(n);
    res := factorial(n);
    write('The factorial of that is: ');
    writeln(res);
end.